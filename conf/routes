GET  /                                          controllers.IndexController.index()
GET  /signup                                    controllers.IndexController.signUp()
GET  /signin                                    controllers.IndexController.signIn()
GET  /user/:userId/dashboard                    controllers.IndexController.dashboard(userId:Long)

POST /user/new                                  controllers.UserController.create()
POST /user/login                                controllers.UserController.login()
GET  /user/logout                               controllers.UserController.logout()


# Map static resources from the /public folder to the /assets URL path
GET  /assets/*file                              controllers.Assets.versioned(path="/public", file: Asset)
